// User function Template for C++

class Solution {
    
private:
    int func(int n)
    {
        if (n%4==1)
        return 1;
        else if (n%4==2)
        return n+1;
        else if(n%4==3)
        return 0;
        else
        return n;
    }
    

  public:
    int findXOR(int l, int r) {
        // complete the function here
        int a = func(l-1);
        int b = func(r);
        return a^b;
        
    }
};
